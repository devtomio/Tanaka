# Reddit Keys
REDDIT_CLIENT_ID= # the client id of your reddit app
REDDIT_CLIENT_SECRET= # the client secret of your reddit app
REDDIT_PASSWORD= # the password of your reddit account

# My Own Bot List
BOTS_ID= # the webhook id of the bot logs
BOTS_TOKEN= # the webhook token of the bot logs

# Test Webhook
TEST_WEBHOOK_ID= # the test webhook id
